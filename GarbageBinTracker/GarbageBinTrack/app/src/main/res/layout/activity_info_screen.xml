<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_infoBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:backgroundTint="#5313BA"
        android:text="Back"
        app:iconTint="#5313BA"
        app:layout_constraintBottom_toTopOf="@+id/btn_deleteSensor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_deleteSensor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#5313BA"
        android:text="Delete Sensor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="121dp"
        android:layout_height="130dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_delete" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.622"
        app:srcCompat="@android:drawable/ic_dialog_info"
        app:tint="#5313BA" /><![CDATA[









    app:layout_constraintStart_toEndOf="@+id/tv_infoTest1/>











    ]]>

    <TextView
        android:id="@+id/tv_infoTest1"
        android:layout_width="135dp"
        android:layout_height="21dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="88dp"
        android:text="Avg Pickup Percent:"
        app:layout_constraintEnd_toStartOf="@+id/tv_infoValue1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/tv_infoTest2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="20dp"
        android:text="# Of Pickups:"
        app:layout_constraintEnd_toStartOf="@+id/tv_infoValue2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_infoTest1" />

    <TextView
        android:id="@+id/tv_infoTest3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        android:text="Last Time Picked Up:"
        app:layout_constraintEnd_toStartOf="@+id/tv_infoValue3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_infoTest2" />

    <TextView
        android:id="@+id/tv_infoValue1"
        android:layout_width="33dp"
        android:layout_height="20dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="136dp"
        android:text="58%"
        app:layout_constraintBottom_toTopOf="@+id/tv_infoTest2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_infoTest1"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0" />

    app:layout_constraintStart_toEndOf="@+id/tv_infoTest2"

    app:layout_constraintStart_toEndOf="@+id/tv_infoTest3"


    <TextView
        android:id="@+id/tv_infoValue2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="160dp"
        android:text="6"
        app:layout_constraintBottom_toTopOf="@+id/tv_infoTest3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_infoTest2"
        app:layout_constraintTop_toBottomOf="@+id/tv_infoValue1"
        app:layout_constraintVertical_bias="0.045" />

    <TextView
        android:id="@+id/tv_infoValue3"
        android:layout_width="141dp"
        android:layout_height="25dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="24dp"
        android:text="No Pickups Registred"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_infoTest3"
        app:layout_constraintTop_toBottomOf="@+id/tv_infoValue2" />

    <TextView
        android:id="@+id/tv_infoName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TrashBin1"
        app:layout_constraintBottom_toTopOf="@+id/tv_infoTest1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Monthly Stats:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.299"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />
</androidx.constraintlayout.widget.ConstraintLayout>